{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fyle - Expense Tracking & Reports",
  "short_name": "Fyle",
  "version": "0.3.2",
  "description": "Automatically track and manage expenses right inside Gmail",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "identity",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/*",
    "*://*.googleusercontent.com/*",
    "https://*.fyle.in/*",
    "https://localhost:1234/*",
    "http://localhost:5000/*"
  ],
  "background": {
    "scripts": [
      "jquery-1.10.2.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "css": [
        "base.css"
      ],
      "js": [
        "jquery-1.10.2.js",
        "inboxsdk.js",
        "loader.js"
      ]
    },
    {
      "matches": [
        "https://mail.google.com/*",
        "https://*.fyle.in/*",
        "https://localhost:1234/*",
        "http://localhost:5000/*"
      ],
      "js": [
        "jquery-1.10.2.js",
        "version.js"
      ]
    },
    {
      "matches": [
        "http://www.fyle.in/*",
        "http://localhost:3000/*"
      ],
      "js": [
        "jquery-1.10.2.js",
        "version.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*"
  ],
  "manifest_version": 2
}