{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save to Google Photos\u2122",
  "version": "10.8.2",
  "manifest_version": 2,
  "description": "Upload images to Google Photos",
  "key": "",
  "permissions": [
    "contextMenus",
    "identity",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "assets/js/buy.js",
      "assets/js/Payments.js",
      "assets/js/ContextMenuUpload.js",
      "assets/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/icon16.png",
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "index.html",
    "chrome_style": true
  },
  "icons": {
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "oauth2": {
    "client_id": "475159336221-25j59rqeoa718k25fe02nvq7ph9iihai.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/photoslibrary"
    ]
  },
  "minimum_chrome_version": "46",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'"
}