{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "1.9",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Wreck It Ralph 2"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Search",
      "keyword": "EZ",
      "encoding": "UTF-8",
      "is_default": true,
      "search_url": "https://popular-themes.com/api/search.php?q={searchTerms}&theme=omni",
      "favicon_url": "https://popular-themes.com/favicon.ico"
    }
  }
}